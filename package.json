  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=configs/webpack/dev.js",
    "prod": "npm run build",
    "build": "npm run clean-dist && cross-env NODE_ENV=production webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run tslint && npm run stylelint",
    "lint:fix": "npm run tslint:fix && npm run stylelint:fix",
    "tslint": "tslint './src/**/*.ts*' --format stylish --force --project tsconfig.json",
    "tslint:fix": "npm run tslint -- --fix",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettify": "prettier --write 'src/**/*.{ts,tsx}'"
  },
